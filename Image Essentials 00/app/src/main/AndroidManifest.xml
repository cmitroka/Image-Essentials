<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mc2techservices.imageessentials_crop_resize_shrink_rotate">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Image Essentials"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">
        <activity android:name="com.mc2techservices.imageessentials_crop_resize_shrink_rotate.InitActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mc2techservices.imageessentials_crop_resize_shrink_rotate.MainActivity"></activity>
        <activity android:name="com.mc2techservices.imageessentials_crop_resize_shrink_rotate.CropResultActivity"></activity>
    </application>

</manifest>